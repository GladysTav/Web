<% if @error.present? %>
  errorSubmit('<%= j @error %>');
<% else %>
  var links;
  '<%= form_for @current_community, url: update_footer_admin2_design_footer_index_path do |form| %>'
    links = "<%= j render 'admin2/design/footer/links', form: form, disabled: @service.plan_footer_disabled? %>";
  '<% end %>'
  $('#simpleList').html(links);
<% end %>
